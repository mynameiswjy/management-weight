page {
    background-color: #F3F3F3;
}

.shipped {
    background:linear-gradient(90deg,rgba(248,206,58,1) 0%,rgba(248,191,66,1) 100%);
    border-radius: 12rpx;
    width: 630;
    padding: 30rpx;
    margin: 20rpx 30rpx;
    align-items: center;
}

.iconyifahuo {
    font-size: 56rpx;
    color: #282828;
    margin-right: 20rpx;
}

.shipped .txt1 {
    font-size: 32rpx;
    color: #282828;
    line-height: 1;
    margin-bottom: 15rpx;
}

.shipped .txt2 {
    font-size: 26rpx;
    color: #666;
    line-height: 1;
}

.express_num {
    background: #fff;
    font-size: 26rpx;
    color: #999;
    line-height: 1;
    padding: 40rpx 30rpx;
}

.express_num .txt3 {
    margin-bottom: 28rpx;
}

.copy {
    width: 64rpx;
    height: 32rpx;
    text-align: center;
    line-height: 32rpx;
    border-radius: 16rpx;
    background-color: #F3F3F3;
    margin-left: 20rpx;
    font-size: 20rpx;
    display: inline-block;
}

.express_info {
    width: 690rpx;
    margin: 0 30rpx 30rpx;
    background-color: #fff;
    border-radius: 12rpx;
    font-size: 26rpx;
    color: #999;
    padding: 50rpx 0 0;
}

.expre_detail {
    max-width: 546rpx;
    line-height: 32rpx;
}

.expres_time {
    line-height: 35rpx;
}

.express_info .item {
    padding-bottom: 45rpx;
    padding-left: 50rpx;
    margin-left: 55rpx;
    position: relative;
}

.express_info .item1 {
    border-left: 1rpx solid #eee;
}

.express_info .item:after {
    content: '';
    width: 16rpx;
    height: 16rpx;
    border-radius: 50%;
    background-color: #CCCCCC;
    position: absolute;
    top: 12rpx;
    left: -8rpx;
}

.express_info .item2 {
    position: relative;
}

.express_info .item2:before {
    content: '';
    width: 1rpx;
    height: 30rpx;
    background-color: #eeeeee;
    position: absolute;
    top: -15rpx;
    left: 0;
}

.express_info .item3 {
    position: relative;
}

.express_info .item3:after {
    content: '';
    width: 22rpx;
    height: 22rpx;
    background-color: #F8CE3A;
    position: absolute;
    border-radius: 50%;
    top: 6rpx;
    left: -11rpx;
}

.express_info .item3:before {
    content: '';
    width: 34rpx;
    height: 34rpx;
    background-color: #FBE38C;
    position: absolute;
    border-radius: 50%;
    top: 0;
    left: -17rpx;
}